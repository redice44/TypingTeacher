{
  "name": "typing-teacher",
  "version": "0.0.0",
  "description": "A game to practice and assess typing proficiency",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "db": "mongod",
    "dev": "webpack --watch",
    "server:dev": "export NODE_ENV=dev && node src/server/index.js",
    "server:dev-windows": "set NODE_ENV=dev&&node src/server/index.js",
    "start": "nodemon --watch src/ --delay 3 --exec \"npm run server:dev\"",
    "start-windows": "nodemon --watch src/ --delay 3 --exec \"npm run server:dev-windows\"",
    "mongo-win": "md mongo-db & \"C:/Program Files/MongoDb/Server/3.2/bin/mongod.exe\" --dbpath mongo-db",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redice44/TypingTeacher.git"
  },
  "authors": [
    "Matt Thomson <red.cataclysm@gmail.com>",
    "Victor Estopinan Rodriguez",
    "Kevin Delamo"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/redice44/TypingTeacher/issues"
  },
  "homepage": "https://github.com/redice44/TypingTeacher#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.14.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ncp": "^2.0.0",
    "node-sass": "^3.10.1",
    "nodemon": "^1.10.2",
    "postcss-loader": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "d3": "^4.3.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "material-ui": "^0.16.0",
    "mongo": "^0.1.0",
    "mongoose": "^4.6.3",
    "mongoose-slug": "^1.3.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "react": "^15.3.2",
    "react-d3": "^0.4.0",
